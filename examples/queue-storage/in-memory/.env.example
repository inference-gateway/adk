# Environment configuration for in-memory queue storage example

# Runtime Environment
ENVIRONMENT=development

# Server Configuration
A2A_SERVER_PORT=8080
A2A_DEBUG=true

# Queue Configuration (In-Memory)
A2A_QUEUE_PROVIDER=memory
A2A_QUEUE_MAX_SIZE=100
A2A_QUEUE_CLEANUP_INTERVAL=30s

# Task Retention (for completed/failed tasks)
A2A_TASK_RETENTION_MAX_COMPLETED_TASKS=50
A2A_TASK_RETENTION_MAX_FAILED_TASKS=25
A2A_TASK_RETENTION_CLEANUP_INTERVAL=5m

# Agent Configuration (for AI-powered examples)
# A2A_AGENT_CLIENT_PROVIDER=openai
# A2A_AGENT_CLIENT_MODEL=gpt-4o-mini
# A2A_AGENT_CLIENT_API_KEY=your_api_key_here

# Capabilities
A2A_CAPABILITIES_STREAMING=true
A2A_CAPABILITIES_PUSH_NOTIFICATIONS=false
