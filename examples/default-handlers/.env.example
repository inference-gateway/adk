# Environment configuration for default-handlers example
ENVIRONMENT=development

# Server configuration
A2A_SERVER_PORT=8080
A2A_DEBUG=true

# Agent capabilities
A2A_CAPABILITIES_STREAMING=true
A2A_CAPABILITIES_PUSH_NOTIFICATIONS=false
A2A_CAPABILITIES_STATE_TRANSITION_HISTORY=false

# Optional: LLM configuration (enables AI-powered responses through the default handlers)
# A2A_AGENT_CLIENT_PROVIDER=
# A2A_AGENT_CLIENT_MODEL=
# A2A_AGENT_CLIENT_API_KEY=
