# Environment Configuration for Artifacts with Default Handlers Example

# General Settings
ENVIRONMENT=development

# A2A Server Configuration
A2A_SERVER_PORT=8080
A2A_DEBUG=true
A2A_CAPABILITIES_STREAMING=true

# LLM Provider Configuration (Optional)
# Uncomment and configure for AI-powered responses

# OpenAI Configuration
# A2A_AGENT_CLIENT_PROVIDER=openai
# A2A_AGENT_CLIENT_MODEL=gpt-3.5-turbo
# A2A_AGENT_CLIENT_API_KEY=your_openai_api_key_here

# Anthropic Configuration
# A2A_AGENT_CLIENT_PROVIDER=anthropic
# A2A_AGENT_CLIENT_MODEL=claude-3-haiku-20240307
# A2A_AGENT_CLIENT_API_KEY=your_anthropic_api_key_here

# Azure OpenAI Configuration
# A2A_AGENT_CLIENT_PROVIDER=azure
# A2A_AGENT_CLIENT_MODEL=gpt-35-turbo
# A2A_AGENT_CLIENT_API_KEY=your_azure_api_key_here
# A2A_AGENT_CLIENT_BASE_URL=https://your-resource.openai.azure.com

# Local/Custom Provider Configuration
# A2A_AGENT_CLIENT_PROVIDER=openai-compatible
# A2A_AGENT_CLIENT_MODEL=your_model_name
# A2A_AGENT_CLIENT_API_KEY=your_api_key_here
# A2A_AGENT_CLIENT_BASE_URL=http://localhost:11434/v1

# Client Configuration
SERVER_URL=http://localhost:8080